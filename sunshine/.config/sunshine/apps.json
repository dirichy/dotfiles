{
  "apps": [
    {
      "auto-detach": true,
      "exclude-global-prep-cmd": false,
      "exit-timeout": 5,
      "image-path": "desktop.png",
      "name": "Desktop",
      "prep-cmd": [
        {
          "do": "/home/dirichy/.local/bin/hyprlua hypr.keyword.monitor(\"HDMI-A-1\",\"preferred\",\"auto-down\",\"1.5\")",
          "undo": "/home/dirichy/.local/bin/hyprlua hypr.keyword.monitor(\"HDMI-A-1\",\"disabled\")"
        }
      ],
      "wait-all": true
    },
    {
      "auto-detach": true,
      "exclude-global-prep-cmd": false,
      "exit-timeout": 5,
      "image-path": "steam.png",
