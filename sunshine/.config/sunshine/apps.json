        {
          "do": "/usr/bin/test",
          "undo": ""
        }
      ],
      "wait-all": true
    },
    {
      "auto-detach": true,
      "cmd": [],
      "elevated": false,
      "exclude-global-prep-cmd": false,
      "exit-timeout": 5,
      "image-path": "",
      "name": "Ipad-only",
